{
  "env": {
    "test": {
      "sourceMaps": "inline",
      "retainLines": true,
      "presets": [
        [
          "es2015"
        ]
      ],
      "plugins": [
        "transform-object-rest-spread",
        [
          "istanbul",
          {
            "include": [
              "packages/*/src/**/*.js",
              "plugins/*/src/**/*.js"
            ]
          }
        ]
      ]
    }
  }
}
