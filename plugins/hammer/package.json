{
  "name": "@qlik/picasso-plugin-hammer",
  "version": "0.40.0",
  "description": "Hammer JS interaction mixin for Picasso",
  "license": "ISC",
  "private": false,
  "publishConfig": {
    "registry": "https://qliktech.jfrog.io/qliktech/api/npm/npm-local/"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:qlik-trial/picasso.js.git"
  },
  "scripts": {
    "build": "exec-closest rollup -c ../../rollup.config.js --name picassoHammer --environment BUILD:production",
    "build:dev": "exec-closest rollup -c ../../rollup.config.js --name picassoHammer",
    "build:watch": "exec-closest rollup -c ../../rollup.config.js --name picassoHammer -w",
    "lint": "exec-closest eslint src test",
    "test": "exec-closest aw -c aw.config.js"
  },
  "files": [
    "/dist"
  ],
  "main": "dist/picasso-hammer.min.js",
  "module": "src/index.js",
  "devDependencies": {
    "exec-closest": "*",
    "test-utils": "*"
  }
}
